<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="15dp"
    tools:context=".HomeActivity">

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:text="用户名"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/tv_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:text="密码"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_name"/>
    <TextView
        android:id="@+id/tv_join_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:text="加入时间"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_password"/>

    <Button
        android:id="@+id/btn_date_to"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="转化时间1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_join_date"/>

    <Button
        android:id="@+id/btn_date_from"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="转化时间2"
        app:layout_constraintLeft_toRightOf="@+id/btn_date_to"
        app:layout_constraintTop_toTopOf="@+id/btn_date_to"/>


</androidx.constraintlayout.widget.ConstraintLayout>